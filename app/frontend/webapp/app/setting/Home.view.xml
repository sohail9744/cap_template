<mvc:View
	controllerName="wisys.frontend.app.setting.Home"
	xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true"
	xmlns="sap.m"
>
	<Page showHeader="false">
		<content>
			<GenericTile
				class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
				header="Workflow Approvers"
				subheader="Approvers Details"
				press="onRouteApprovers"
			>
				<TileContent
					unit="Workflow"
					footer="Admin"
				>
					<ImageContent src="sap-icon://people-connected" />
				</TileContent>
			</GenericTile>
			<GenericTile
				class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
				header="Master Data"
				subheader="Details"
				press="onRouteMasterData"
			>
				<TileContent
					unit="Master Data"
					footer="Admin"
				>
					<ImageContent src="sap-icon://database" />
				</TileContent>
			</GenericTile>
			<GenericTile
				class="sapUiSmallMarginBegin sapUiTinyMarginTop tileLayout"
				header="Reassign Request"
				subheader="Support Service Request"
				press="onRouteReassign"
			>
				<TileContent
					unit="Requests"
					footer="Reassign"
				>
					<ImageContent src="sap-icon://citizen-connect" />
				</TileContent>
			</GenericTile>
		</content>
	</Page>
</mvc:View>